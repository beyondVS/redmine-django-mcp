        * `archive_project` - archive_project
        * `delete_project` - delete_project
        * `manage_members` - manage_members
        * `view_issues` - view_issues
        * `create_issue` - create_issue
        * `edit_issue` - edit_issue
        * `delete_issue` - delete_issue
        * `add_issue_notes` - add_issue_notes
        * `view_wiki_pages` - view_wiki_pages
        * `edit_wiki_pages` - edit_wiki_pages
        * `delete_wiki_pages` - delete_wiki_pages
        * `log_time` - log_time
        * `view_time_entries` - view_time_entries
        * `edit_time_entries` - edit_time_entries
    Project:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
        identifier:
          type: string
          pattern: ^[a-z0-9_\-]+$
          maxLength: 255
        description:
          type: string
        homepage:
          type: string
          format: uri
          maxLength: 200
        is_public:
          type: boolean
        parent:
          type: integer
          readOnly: true
          nullable: true
        parent_id:
          type: integer
          writeOnly: true
          nullable: true
        status:
          allOf:
          - $ref: '#/components/schemas/StatusEnum'
          readOnly: true
        created_on:
          type: string
          format: date-time
          readOnly: true
        updated_on:
          type: string
          format: date-time
          readOnly: true
        inherit_members:
          type: boolean
          writeOnly: true
          default: false
        enabled_modules:
          type: array
          items:
            type: string
          writeOnly: true
        modules:
          type: string
          readOnly: true
      required:
      - created_on
      - id
      - identifier
      - modules
      - name
      - parent
      - status
      - updated_on
    Role:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
        permissions:
          type: array
          items:
            $ref: '#/components/schemas/PermissionsEnum'
        issues_visibility:
          $ref: '#/components/schemas/IssuesVisibilityEnum'
      required:
      - id
      - name
      - permissions
    Setting:
      type: object
      properties:
        name:
          type: string
          maxLength: 255
        value:
          type: string
        updated_on:
          type: string
          format: date-time
          readOnly: true
      required:
      - name
      - updated_on
    StatusEnum:
      enum:
      - 1
      - 5
      - 9
      type: integer
      description: |-
        * `1` - Active
        * `5` - Closed
        * `9` - Archived
    TimeEntry:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        project:
          type: integer
        issue:
          type: integer
          nullable: true
        user:
          type: integer
        hours:
          type: number
          format: double
        spent_on:
          type: string
          format: date
        activity:
          type: integer
          readOnly: true
        activity_id:
          type: integer
          writeOnly: true
        comments:
          type: string
          maxLength: 1024
        created_on:
          type: string
          format: date-time
          readOnly: true
        updated_on:
          type: string
          format: date-time
          readOnly: true
      required:
      - activity
      - created_on
      - hours
      - id
      - project
      - spent_on
      - updated_on
    TimeEntryActivity:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
        is_default:
          type: boolean
        active:
          type: boolean
        position:
          type: integer
          maximum: 9223372036854775807
          minimum: -9223372036854775808
          format: int64
      required:
      - id
      - name
    Tracker:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
        default_status:
          type: integer
          nullable: true
        position:
          type: integer
          maximum: 9223372036854775807
          minimum: -9223372036854775808
          format: int64
      required:
      - id
      - name
    User:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        username:
          type: string
          title: 사용자 이름
          description: 150자 이하 문자, 숫자 그리고 @/./+/-/_만 가능합니다.
          pattern: ^[\w.@+-]+$
          maxLength: 150
        email:
          type: string
          format: email
          title: 이메일 주소
          maxLength: 254
        first_name:
          type: string
          title: 이름
          maxLength: 150
        last_name:
          type: string
          title: 성
          maxLength: 150
        is_active:
          type: boolean
          title: 활성
          description: 이 사용자가 활성화되어 있는지를 나타냅니다. 계정을 삭제하는 대신 이것을 선택 해제하세요.
        is_staff:
          type: boolean
          title: 스태프 권한
          description: 사용자가 관리사이트에 로그인이 가능한지를 나타냅니다.
        is_superuser:
          type: boolean
          title: 최상위 사용자 권한
          description: 해당 사용자에게 모든 권한을 허가합니다.
        password:
          type: string
          writeOnly: true
        language:
          type: string
          maxLength: 10
        date_joined:
          type: string
          format: date-time
          readOnly: true
          title: 등록일
        last_login:
          type: string
          format: date-time
          readOnly: true
          nullable: true
          title: 마지막 로그인
      required:
      - date_joined
      - id
      - last_login
      - password
      - username
    WikiContent:
      type: object
      properties:
        text:
          type: string
        comments:
          type: string
          maxLength: 1024
        version:
          type: integer
          readOnly: true
        updated_on:
          type: string
          format: date-time
          readOnly: true
        author:
          type: integer
          readOnly: true
          nullable: true
      required:
      - author
      - text
      - updated_on
      - version
    WikiPage:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        wiki:
          type: integer
          readOnly: true
        title:
          type: string
          maxLength: 255
        parent:
          type: integer
          nullable: true
        created_on:
          type: string
          format: date-time
          readOnly: true
        content:
          allOf:
          - $ref: '#/components/schemas/WikiContent'
          readOnly: true
        text:
          type: string
          writeOnly: true
        comments:
          type: string
          writeOnly: true
        version:
          type: integer
          writeOnly: true
      required:
      - content
      - created_on
      - id
      - title
      - wiki
    WorkflowTransition:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        tracker:
          type: integer
        role:
          type: integer
        old_status:
          type: integer
        new_status:
          type: integer
      required:
      - id
      - new_status
      - old_status
      - role
      - tracker
  securitySchemes:
    cookieAuth:
      type: apiKey
      in: cookie
      name: sessionid
    jwtAuth:
      type: http
      scheme: bearer
      bearerFormat: JWT
